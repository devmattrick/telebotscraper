{{#each imports as |imp impKey|}}
import { {{imp.name}} } from "../{{imp.category}}/{{imp.name}}";
{{/each}}


/**
 * {{{schema.description}}}
 */
export type {{schema.name}} = {
  {{#each schema.fields as |field fieldKey|}}
  /**
   * {{{field.description}}}
   */
  {{field.name}}{{#if field.optional}}?{{/if}}: {{field.type}};

  {{/each}}
}
