  {{#each schema.fields as |field fieldKey|}}
  /**
  * {{field.description}}
  */
  readonly {{field.name}}{{#if field.optional}}?{{/if}}: {{field.type}};

  {{/each}}
  constructor(obj: { {{#each schema.fields as |field fieldKey|}}{{#unless field.optional}}{{field.name}}: {{field.type}}, {{/unless}}{{/each}}{{#each schema.fields as |field fieldKey|}}{{#if field.optional}}{{field.name}}?: {{field.type}}, {{/if}}{{/each}} } ) {
    {{#each schema.fields as |field fieldKey|}}
    {{{getDeclaration field.name field.type}}}
    {{/each}}
  }
 
}
